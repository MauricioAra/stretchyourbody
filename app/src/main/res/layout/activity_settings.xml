    </LinearLayout>

    <include
        android:id="@+id/toobar"
        layout="@layout/toolbar"
        tools:layout_editor_absoluteX="-32dp"
        tools:layout_editor_absoluteY="-30dp" />


</android.support.constraint.ConstraintLayout>
